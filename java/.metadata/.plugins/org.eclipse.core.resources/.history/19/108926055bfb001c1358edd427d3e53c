create table user
(ID serial ,
MAIL varchar(255) not null,
PASSWORD varchar(255) not null,
REG_DATE date not null,
UPD_DATE date not null,
primary key (MAIL));

create table content(
	ID serial ,
	TITLE varchar(60) not null,
	CONTENT text ,
	REG_DATE date not null,
	UPD_DATE date not null,
	primary key (ID)
);